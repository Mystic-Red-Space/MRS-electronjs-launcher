{
  "name": "mrs-electron-launcher",
  "version": "0.1.0",
  "description": "Minecraft modpack launcher of MRS Minecraft server",
  "repository": "https://github.com/Team-IF/MRS-electronjs-launcher",
  "author": "Team-IF",
  "license": "AGPL-3.0-or-later",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "testbuild": "electron-builder --dir",
    "build": "electron-builder"
  },
  "build": {
    "appId": "team.if.mrslauncher",
    "productName": "MRS Minecraft Launcher",
    "win": {
      "icon": "logo.ico"
    },
    "nsis": {
      "oneClick": true,
      "installerIcon": "logo.ico",
      "uninstallerIcon": "logo.ico"
    }
  },
  "dependencies": {
    "axios": "^0.19.2",
    "discord-rpc": "^3.1.0",
    "minecraft-launcher-core": "^3.12.0",
    "node-machine-id": "^1.1.12",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "electron": "^8.0.1",
    "electron-builder": "^22.3.2"
  }
}
